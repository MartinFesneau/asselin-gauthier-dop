<%= link_to project_path(project) do  %>
  <div class="project-card" style="background-image: url('<%= cl_image_path project.photos.first.key %>'), linear-gradient(rgba(0,0,0,0.1), rgba(0,0,0,0.1));" data-aos="fade-in">
    <div class="content">
      <p><%= project.name %></p>
      <p><%=  project.artist %></p>
    </div>
  </div>
<% end %>